ii	myprogram.c	/^FILE	*ii;										\/* input-file pointer *\/$/;"	v	language:C
ii_file_name	myprogram.c	/^char	*ii_file_name = "";		\/* input-file name    *\/$/;"	v	language:C
main	myprogram.c	/^int main ( int argc, char *argv[] )$/;"	f	language:C	signature:( int argc, char *argv[] )
